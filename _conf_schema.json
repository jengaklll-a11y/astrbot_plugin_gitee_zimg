{
    "api_key": {
        "description": "Gitee API Key (支持多Key)",
        "type": "list",
        "default": [],
        "obvious_hint": false
    },
    "size": {
        "description": "图片分辨率",
        "type": "string",
        "default": "1:1 (2048×2048)",
        "options": [
            "1:1 (2048×2048)",
            "3:4 (1536×2048)",
            "4:3 (2048×1536)",
            "2:3 (1360×2048)",
            "3:2 (2048×1360)",
            "9:16 (1152×2048)",
            "16:9 (2048×1152)"
        ]
    },
    "num_inference_steps": {
        "description": "推理步数 (1-50)",
        "type": "int",
        "default": 9,
        "slider": {
            "min": 1,
            "max": 50,
            "step": 1
        }
    },
    "retention_hours": {
        "description": "临时图片保留时长/小时 (0为不删除)",
        "type": "float",
        "default": 1.0,
        "slider": {
            "min": 0,
            "max": 72,
            "step": 0.5
        }
    }
}


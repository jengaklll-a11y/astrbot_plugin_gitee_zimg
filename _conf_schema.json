{
    "base_url": {
        "description": "Gitee API 地址",
        "type": "string",
        "default": "https://ai.gitee.com/v1"
    },
    "api_key": {
        "description": "Gitee API Key",
        "type": "list",
        "default": [],
        "obvious_hint": false
    },
    "model": {
        "description": "绘图模型",
        "type": "string",
        "default": "z-image-turbo"
    },
    "size": {
        "description": "图片分辨率",
        "type": "string",
        "default": "2048x2048"
    },
    "num_inference_steps": {
        "description": "推理步数",
        "type": "int",
        "default": 9,
        "hint": "范围1-50，默认为9"
    },
    "retention_hours": {
        "description": "临时图片保留时长 (小时)",
        "type": "float",
        "default": 1.0,
        "hint": "填 0 则不自动删除"
    }
}

